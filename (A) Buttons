import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();

        while (t-- > 0) {
            int a = sc.nextInt();
            int b = sc.nextInt();
            int c = sc.nextInt();

            if((c%2!=0 && b>a) || (c%2==0 && b>=a)){
                System.out.println("Second");
            }else{
                System.out.println("First");
            }
        }
    }
}
