
#include <bits/stdc++.h>
using namespace std;
 
 
int main(){
 
	ios::sync_with_stdio(false);
	cin.tie(nullptr);
	
	int t;
	cin>>t;
	
	while(t--){
		string s;

		cin>>s;

		unordered_map<char, int>m;
		int count=0;

		for(char c:s){
			m[c]++;
		}
		int f=m['F'];
		int l=m['T'];
		int n=m['N'];

		s.erase(remove(s.begin(), s.end(), 'F'), s.end());
        s.erase(remove(s.begin(), s.end(), 'T'), s.end());
        s.erase(remove(s.begin(), s.end(), 'N'), s.end());

		int i=1;

		while(i<=l){
			s.push_back('T');
			i++;
		}
		i=1;
	
		while(i<=n){
			s.push_back('N');
			i++;
		}
		i=1;
		while(i<=f){
			s.push_back('F');
			i++;
		}


		cout<<s<<"\n";

	}
}
