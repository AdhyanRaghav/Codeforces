#include <bits/stdc++.h>
using namespace std;

int main(){

	ios::sync_with_stdio(false);
	cin.tie(nullptr);

	int t;
	cin>>t;

	while(t--){

		int n,m;

		cin>>n>>m;
		string x;
		string s;

		cin>>x;
		cin>>s;

		int count=0;
		bool rsult=false;

		if(x==s){
			rsult=true;
		}else{
			while(x.size()<=50){
				if(x.find(s)!=string::npos){
					rsult=true;
					break;
				}
				x.append(x);
				count++;
			}
		}
		if(rsult) cout<<count<<"\n";
		else cout<<-1<<"\n";
	}
}
