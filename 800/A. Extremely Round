import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();

        while (t-- > 0) {
            int n = sc.nextInt();
            String s = String.valueOf(n);
            int len = s.length();

            // All extremely round numbers with fewer digits
            int count = (len - 1) * 9;

            // Add numbers with same digits as n
            count += s.charAt(0) - '0';

            System.out.println(count);
        }
    }
}
