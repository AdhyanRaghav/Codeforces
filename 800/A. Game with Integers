import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
 
        while (t-- > 0) {
 
            int n=sc.nextInt();
            boolean flag=false;
 
            for(int i=0;i<10;i++){
                if(Math.abs(n-1)%3==0 || Math.abs(n+1)%3==0){
                    flag=true;
                    System.out.println("First");
                    break;
                }
            }
            if(!flag){
                System.out.println("Second");
            }
 
        }
    }
}
