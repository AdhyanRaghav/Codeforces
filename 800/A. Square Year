import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
 
        while (t-- > 0) {
            String s = sc.next();
 
            double str = Integer.parseInt(s);
            int a = (int) Math.sqrt(str);
 
            if(a * a == Integer.parseInt(s)){
                System.out.println("0 "+a);
            }else{
                System.out.println("-1");
            }
        }
    }
}
