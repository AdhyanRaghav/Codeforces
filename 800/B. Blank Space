
#include <bits/stdc++.h>
using namespace std;


int main(){

	ios::sync_with_stdio(false);
	cin.tie(nullptr);

	int t;
	cin>>t;

	while(t--){

		int n;
		cin>>n;

		vector<int>arr(n);

		int count=0;
		int Max=0;

		for(int i=0;i<arr.size();i++){
			cin>>arr[i];
			if(arr[i]==0) {
				count++;
				Max=max(count,Max);
			}else {
				count=0;
			}
		}

		cout<<Max<<"\n";

	}
}
