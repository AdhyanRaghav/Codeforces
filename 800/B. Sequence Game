
#include <bits/stdc++.h>
using namespace std;

int main(){

	ios::sync_with_stdio(false);
	cin.tie(nullptr);

	int t;
	cin>>t;

	while(t--){
		int n;
		cin>>n;

		vector<int>arr(n);
		vector<int>arr1;

		for (int i = 0; i <n;i++)
		{
			cin>>arr[i];
			if(i==0 || arr[i-1]<=arr[i]){
				arr1.push_back(arr[i]);
			}else{
				arr1.push_back(1);
				arr1.push_back(arr[i]);
			}
		}

		

		// for(int i=0;i<arr.size();i++){
		// 	if(i==0 || arr[i-1]<=arr[i]){
		// 		arr1.push_back(arr[i]);
		// 	}else{
		// 		arr1.push_back(1);
		// 		arr1.push_back(arr[i]);
		// 	}
		// }

		int m=arr1.size();

		cout<<m<<"\n";

		for (int i = 0; i < arr1.size();i++)
		{
			cout<<arr1[i]<<" ";
		}

		cout<<"\n";
	}
}
