#include <bits/stdc++.h>
using namespace std;


int main(){

	ios::sync_with_stdio(false);
	cin.tie(nullptr);

	int t;
	cin>>t;

	while(t--){

		int n;
		cin>>n;

		vector<int>arr(n);

		vector<int> freq(102, 0);
        for (int i = 0; i < n; i++) {
           int x;
           cin >> x;
           freq[x]++;
        }

        int Zero = 0;
        while (Zero < 102 && freq[Zero] > 0) Zero++;

        int LessTwo = 0;
        while (LessTwo < 102 && freq[LessTwo] >= 2) LessTwo++;

        int a = max(Zero, LessTwo);
        cout << a << "\n";
    }
}
