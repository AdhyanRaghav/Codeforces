#include <bits/stdc++.h>
using namespace std;

string repeat(const string& str, int times) {
    	string result;
    	for (int i = 0; i < times; i++) result += str;
   			return result;
}

int main(){

	ios::sync_with_stdio(false);
	cin.tie(nullptr);


	int t;
	cin >> t;

	while(t--){
		string s;
	    cin >> s;

		int aboc=0;
		int baoc=0;

		for(int i=0;i+1<(int)s.size();i++){
			if(i+1<=s.size() && s[i]=='a' && s[i+1]=='b'){
				aboc++;
			}if(i+1<=(int)s.size() && s[i]=='b' && s[i+1]=='a'){
				baoc++;
			}
		}

		if(aboc==baoc){
			cout << s <<"\n";
		}
		else{
			if(s.back()=='a') s.back()='b';
			else s.back()='a';
			cout << s <<"\n";
		}

	}
}
