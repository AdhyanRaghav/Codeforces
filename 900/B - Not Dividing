
#include <bits/stdc++.h>
using namespace std;


int main(){

	ios::sync_with_stdio(false);
	cin.tie(nullptr);

	int t;
	cin>>t;

	while(t--){
		int n;
		cin>>n;

		vector<int>arr(n);
		int Max=0;

		for(int i=0;i<n;i++){
			cin>>arr[i];
		}
		for(int i=1;i<arr.size();i++){
			if(arr[i-1]==1){
				arr[i-1]++;
			}
			while(arr[i] % arr[i-1] == 0){
   				arr[i]++;
			}
		}
		for(int i=1;i<arr.size();i++){
			if(arr[i-1]==1){
				arr[i-1]++;
			}
			while(arr[i] % arr[i-1] == 0){
   				arr[i]++;
			}
		}
		for(int i=0;i<arr.size();i++){
			cout<<arr[i]<<" ";
		}

		cout<<"\n";
	}
}
